 <!--  通过切入点编码获取bean -->
  <select id="getBeanByExecutePointCode" resultMap="BaseResultMap" parameterType="com.idcq.appserver.dto.programconfig.ExecutePointDto" >
	SELECT 
	    pc.program_config_id,
	    pc.spring_bean,
	    pc.program_interface,
	    pc.module_code,
	    pc.program_code,
	    pc.program_type, 
        pc.program_name, 
        pc.program_desc
    FROM 1dcq_execute_point ep
	INNER JOIN  1dcq_execute_detail ed
	ON
	ep.execute_point_id = ed.execute_point_id
	INNER JOIN 1dcq_program_config pc
	ON
	pc.program_config_id = ed.program_config_id
	
	WHERE
	1=1
	<if test="code != null">
	    AND ep.code = #{code}
	</if>
	<if test="moduleCode != null">
		AND ep.module_code = #{moduleCode}
	</if>
    limit 1
  </select>
  
