	<select id="getLauncherIconListByType" resultMap="BaseResultMap"
		parameterType="java.util.Map">
		SELECT
		<include refid="Base_Column_List2" />
		FROM
		1dcq_shop sh
		INNER JOIN 1dcq_launcher_icon_relation sl
		ON sh.column_id=sl.column_id 
		INNER JOIN 1dcq_launcher_icon t1
		ON sl.launcher_icon_id=t1.ID
		INNER JOIN 1dcq_attachment_relation t2 
			ON t1.ID = t2.biz_id AND t2.biz_type = 12
		INNER JOIN 1dcq_attachment t3 
			ON t2.attachement_id = t3.attachement_id
		WHERE  t1.launcher_type = ${launcherType} AND sh.shop_id=#{shopId};
	</select>
